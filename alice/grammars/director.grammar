root:
    [$Find? $Director $Of? $Film]
    $Ellipsis

slots:
    film:
        source: $Film

filler:
    алиса

$Ellipsis:
    $LeftFiller [$Director $This?]

$LeftFiller:
    а

$Film:
    .+

$Of:
    %lemma
    фильм
    кино

$Director:
    %lemma
    кто? снял
    кто? автор
    кто? режиссер

$Find:
    %lemma
    назови
    подскажи
    поищи

$This:
    %lemma
    этого фильма
    его
    это
